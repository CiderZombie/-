# docker 로 해보자
docker run --name snipedb -d -e MYSQL_ROOT_PASSWORD=1234 -e MYSQL_DATABASE=snipe -e MYSQL_USER=snipe -e MYSQL_PASSWORD=12345 -e TZ=Asia/Seoul -p 3306:3306 mysql
